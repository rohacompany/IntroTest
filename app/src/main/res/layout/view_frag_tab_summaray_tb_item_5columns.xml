<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="50dp"
    android:paddingRight="1dp">

    <TextView
        android:id="@+id/title"
        style="@style/frag_tab_summary_tablerow_textview_title"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:text="고창" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tv1"
            style="@style/frag_tab_summary_tablerow_textview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="2" />

        <TextView
            android:id="@+id/tv2"
            style="@style/frag_tab_summary_tablerow_textview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="0" />

        <TextView
            android:id="@+id/tv3"
            style="@style/frag_tab_summary_tablerow_textview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="2" />

        <TextView
            android:id="@+id/tv4"
            style="@style/frag_tab_summary_tablerow_textview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="2" />

        <TextView
            android:id="@+id/tv5"
            style="@style/frag_tab_summary_tablerow_textview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="2" />


    </LinearLayout>
</TableRow>