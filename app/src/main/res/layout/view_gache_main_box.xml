<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    android:foreground="?android:attr/selectableItemBackground"
    android:minHeight="?android:attr/listPreferredItemHeight"
    android:orientation="horizontal"
    card_view:cardBackgroundColor="#fff"
    card_view:cardCornerRadius="2dp"
    card_view:contentPadding="16dp"
    card_view:cardElevation="6dp"
    card_view:cardUseCompatPadding="true"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/view_nongga_main_container"
        android:animationCache="true"
        >

        <TextView
            android:id="@+id/table_result_search_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="개체결과"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/Black" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/LightGrey"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="축협명" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/nongga_id"
                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="지역(시군구)" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="1dp"
                android:id="@+id/chukju_name"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="지역(읍면동)" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/nongga_code"
                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="농가코드" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="1dp"
                android:id="@+id/bangyok_name"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="농가명" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="목장명" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="1dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="귀표번호" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/bangyok_date"
                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="성별" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="1dp"
                android:id="@+id/bangyok_times"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="생년월일" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="월령" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_marginRight="1dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="등록구분" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="개체상태" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_marginRight="1dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="계대" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="어미귀표번호" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_marginRight="1dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="씨수소" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="외조부" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_marginRight="1dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="외증조부" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:text="개체구분" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_marginRight="1dp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="?android:attr/listPreferredItemHeight"
            android:orientation="horizontal"
            android:background="@color/LightGrey"
            >

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp"
                android:text="개체관리번호" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp"
                />

            <TextView
                style="@style/cardview_et_label_header"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp"
                android:text="목장주소" />

            <TextView
                style="@style/cardview_et_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"
                />
        </LinearLayout>

    </LinearLayout>


</android.support.v7.widget.CardView>
